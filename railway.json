{
  "build": {
    "root": "server",
    "builder": "NIXPACKS",
    "nixpacksConfig": {
      "phases": {
        "install": {
          "cmds": ["npm install"]
        },
        "start": {
          "cmd": "npm start"
        }
      },
      "language": "node",
      "env": {
        "NODE_ENV": "production",
        "PORT": "$PORT"
      }
    }
  },
  "name": "mern-memories09",
  "region": "us-east-1"
}
